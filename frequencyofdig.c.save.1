#include<stdio.h>
int main()
{
int n,r,i;
int arr[10]={0};
scanf("%d",&n);
while(n>0)
{
r=n%10;
if(r==0)
{arr[0]++;}
else if(r==1)
{arr[1]++;}
else if(r==2)
{arr[2]++;}
else if(r==3)
{arr[3]++;}
else if(r==4)
{arr[4]++;}
else if(r==5)
{arr[5]++;}
else if(r==6)
{arr[6]++;}
else if(r==7)
{arr[7]++;}
else if(r==8)
{arr[8]++;}
else if(r==9)
{arr[9]++;}
n=n/10;
}
for(i=0;i<10;i++)
{
printf("%d : %d \n",i,arr[i]);
}
return 0;
}
